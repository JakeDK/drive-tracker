{
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./app/*",
        "./*"
      ],
      "~/*": [
        "*"
      ]
    },
    "baseUrl": "."
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts",
    "declarations.d.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base"
}
